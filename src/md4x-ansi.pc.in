prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@
libdir=${exec_prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@

Name: @PROJECT_NAME@ ANSI renderer
Description: Markdown to ANSI terminal output converter library.
Version: @PROJECT_VERSION@
URL: @PROJECT_URL@

Requires: md4x = @PROJECT_VERSION@
Libs: -L${libdir} -lmd4x-ansi
Cflags: -I${includedir}
